{
	"name": "kokobi",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "bunx --bun vite dev",
		"build": "bunx --bun vite build",
		"start": "bunx drizzle-kit migrate && bun run ./.output/server/index.mjs",
		"generate-routes": "tsr generate",
		"watch-routes": "tsr watch",
		"generate-auth": "bunx --bun @better-auth/cli@latest generate --output src/server/db/auth.ts",
		"typecheck": "tsc --noEmit",
		"db:pull": "dotenv -f .env -- litestream restore -o ./persist/sqlite.db s3://kokobi-prod/db"
	},
	"dependencies": {
		"@aws-sdk/client-sesv2": "^3.883.0",
		"@aws-sdk/credential-providers": "^3.883.0",
		"@libsql/client": "^0.15.15",
		"@orpc/client": "^1.8.7",
		"@orpc/openapi": "^1.8.7",
		"@orpc/server": "^1.8.7",
		"@orpc/tanstack-query": "^1.8.7",
		"@orpc/zod": "^1.8.7",
		"@react-pdf/renderer": "^4.3.0",
		"@tailwindcss/postcss": "^4.1.13",
		"@tanstack/react-form": "^1.19.4",
		"@tanstack/react-query": "^5.87.1",
		"@tanstack/react-query-devtools": "^5.87.3",
		"@tanstack/react-router": "^1.131.36",
		"@tanstack/react-router-with-query": "^1.130.17",
		"@tanstack/react-start": "^1.131.36",
		"@tanstack/react-table": "^8.21.3",
		"@tanstack/router-plugin": "^1.131.36",
		"better-auth": "^1.3.8",
		"class-variance-authority": "^0.7.1",
		"cloudflare": "^5.0.0",
		"clsx": "^2.1.1",
		"dotenv": "^17.2.2",
		"drizzle-kit": "^0.31.4",
		"drizzle-orm": "^0.44.5",
		"drizzle-zod": "^0.8.3",
		"export-to-csv": "^1.4.0",
		"fast-xml-parser": "^5.2.5",
		"lucide-react": "^0.542.0",
		"postcss": "^8.5.6",
		"radix-ui": "^1.4.3",
		"react": "19.1.1",
		"react-dom": "19.1.1",
		"recharts": "2",
		"sonner": "^2.0.7",
		"tailwind-merge": "^3.3.1",
		"tailwindcss-animate": "^1.0.7",
		"unzipit": "^1.4.3",
		"zod": "^4.1.5"
	},
	"devDependencies": {
		"@tailwindcss/vite": "^4.1.13",
		"@tanstack/eslint-plugin-router": "^1.131.2",
		"@tanstack/react-router-ssr-query": "^1.131.36",
		"@types/aws-lambda": "8.10.152",
		"@types/bun": "^1.2.21",
		"@types/node": "^24.3.1",
		"@types/react": "^19.1.12",
		"@types/react-dom": "^19.1.9",
		"@vitejs/plugin-react": "^5.0.2",
		"autoprefixer": "^10.4.21",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-unused-imports": "^4.2.0",
		"prettier": "^3.6.2",
		"prettier-plugin-tailwindcss": "^0.6.14",
